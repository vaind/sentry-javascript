{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build:bundle",
          "build:pack",
          "build:tarball",
          "build:types"
        ]
      }
    }
  },
  "targetDefaults": {
    "build:bundle": {
      "dependsOn": ["^build:types"],
      "outputs": [
        "{projectRoot}/build/bundles"
      ]
    },
    "build:tarball": {
      "dependsOn": [
        "build:pack",
        "^build:pack",
        "^build:types"
      ],
      "outputs": [
        "{projectRoot}/build"
      ]
    },
    "build:pack": {
      "dependsOn": [
        "^build:pack",
        "^build:types"
      ],
      "outputs": [
        "{projectRoot}/build/npm",
        "{projectRoot}/build/esm",
        "{projectRoot}/build/cjs"
      ]
    },
    "build:types": {
      "dependsOn": [
        "^build:pack",
        "^build:types"
      ],
      "outputs": [
        "{projectRoot}/build/types",
        "{projectRoot}/build/npm/types"
      ]
    }
  },
  "targets": {
    "@sentry/serverless": {
      "build:bundle": {
        "dependsOn": [],
        "outputs": [
          "{projectRoot}/build/aws"
        ]
      }
    },
    "@sentry/utils": {
      "build:pack": {
        "dependsOn": [
          "^build:pack",
          "^build:types"
        ],
        "outputs": [
          "{projectRoot}/build/esm",
          "{projectRoot}/build/cjs",
          "{projectRoot}/esm",
          "{projectRoot}/cjs"
        ]
      }
    }
  }
}
